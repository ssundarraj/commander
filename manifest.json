{
  "manifest_version": 2,

  "name": "Commander",
  "description": "Command palette for Chrome.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Commander"
  },
  "permissions":[
    "tabs"
  ],
  "commands":{
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+K",
        "mac": "Command+Shift+K",
        "chromeos": "Ctrl+Shift+K",
        "linux": "Ctrl+Shift+K"
      }
   }
  }
}